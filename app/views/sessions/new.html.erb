<h1>Login</h1>
<%= form_with url: '/login' do |form| %>
  <div>
    <%= form.label :email %> <br />
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password %> <br />
    <%= form.password_field :password %>
  </div>
  <%= form.submit 'Login' %>
<% end %>
<%= link_to 'Sign up', signup_path %>